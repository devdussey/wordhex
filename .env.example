# Database Configuration
DATABASE_URL="postgresql://user:password@host:port/database"

# Discord Configuration
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
DISCORD_BOT_TOKEN=your_discord_bot_token

# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend URL
FRONTEND_URL=http://localhost:5173

# Error Webhook Configuration

# Discord Webhook
ERROR_WEBHOOK_DISCORD_URL=https://discord.com/api/webhooks/your_webhook_url
ERROR_WEBHOOK_DISCORD_ENABLED=true
ERROR_WEBHOOK_DISCORD_MIN_SEVERITY=MEDIUM

# Slack Webhook
ERROR_WEBHOOK_SLACK_URL=https://hooks.slack.com/services/your_webhook_url
ERROR_WEBHOOK_SLACK_ENABLED=false
ERROR_WEBHOOK_SLACK_MIN_SEVERITY=MEDIUM

# Generic Webhook (any HTTP endpoint)
ERROR_WEBHOOK_GENERIC_URL=https://your-custom-endpoint.com/webhooks/errors
ERROR_WEBHOOK_GENERIC_ENABLED=false
ERROR_WEBHOOK_GENERIC_MIN_SEVERITY=HIGH

# Webhook Configuration Notes:
# - Set ENABLED to true/false to enable/disable each webhook
# - MIN_SEVERITY options: LOW, MEDIUM, HIGH, CRITICAL
# - Only errors at or above the minimum severity will be sent
# - Multiple webhooks can be enabled simultaneously
# - Leave URL empty or remove the variable to disable a webhook type

# Frontend (Vite) Configuration
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="public-anon-key"
VITE_DISCORD_CLIENT_ID="0000000000000000000"
VITE_SERVER_ID="dev-server-123"
VITE_REALTIME_PROVIDER="websocket"
VITE_ALLOW_CROSS_ORIGIN_API="false"
VITE_API_URL="https://api.your-domain.com"
VITE_WS_URL="wss://ws.your-domain.com"
VITE_DEBUG="false"
